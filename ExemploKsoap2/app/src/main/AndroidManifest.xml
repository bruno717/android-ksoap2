<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.brunooliveira.exemploksoap2">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".PrimaryDataActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".ArrayStringsActivity"
            android:label="@string/menu_array_strings" />

        <activity
            android:name=".AlunoActivity"
            android:label="@string/menu_aluno" />

        <activity
            android:name=".ArrayAlunosActivity"
            android:label="@string/menu_array_alunos" />

    </application>

</manifest>
